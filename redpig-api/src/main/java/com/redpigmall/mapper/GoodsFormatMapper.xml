	<!--database table column names-->
	<sql id="Base_Column_List">
		id,addTime,deleteStatus,gf_name,gf_type,gf_content,gf_store_id,gf_cat
	</sql>
	<sql id="whereStatement">
		<if test="id != null">and id = #{id,jdbcType=BIGINT}</if>
		<if test="addTime != null">and addTime = #{addTime,jdbcType=TIMESTAMP}</if>
		<if test="add_Time_more_than_equal != null">and addTime <![CDATA[ >= ]]> #{add_Time_more_than_equal,jdbcType=TIMESTAMP}</if>
		<if test="add_Time_less_than_equal != null">and addTime <![CDATA[ <= ]]> #{add_Time_less_than_equal,jdbcType=TIMESTAMP}</if>
		<if test="deleteStatus != null">and deleteStatus = #{deleteStatus,jdbcType=INTEGER}</if>
		<if test="gf_name != null">and gf_name = #{gf_name,jdbcType=VARCHAR}</if>
		<if test="gf_type != null">and gf_type = #{gf_type,jdbcType=INTEGER}</if>
		<if test="gf_content != null">and gf_content = #{gf_content,jdbcType=VARCHAR}</if>
		<if test="gf_store_id != null">and gf_store_id = #{gf_store_id,jdbcType=BIGINT}</if>
		<if test="gf_cat != null">and gf_cat = #{gf_cat,jdbcType=BIGINT}</if>
	</sql>
	<sql id="ifStatement">
		gf_name= #{gf_name},
		gf_type= #{gf_type},
		gf_content= #{gf_content},
		gf_store_id= #{gf_store_id},
		gf_cat= #{gf_cat},</sql>
	<select id="selectByPrimaryKey" resultType="com.redpigmall.domain.GoodsFormat" resultMap="BaseResultMap" parameterType="java.lang.Long">
		select 
		<include refid="Base_Column_List"/> 
		from redpigmall_goods_format 
		where id = #{id,jdbcType=BIGINT}
	</select>
	<select id="queryPageList" resultType="com.redpigmall.domain.GoodsFormat" resultMap="BaseResultMap" parameterType="map">
		select
		<include refid="Base_Column_List"/> 
		from redpigmall_goods_format obj 
		where 1=1 
		<include refid="whereStatement"/> 
				order by obj.addTime desc
		<if test="currentPage !=null and pageSize !=null"> 
				limit #{currentPage},#{pageSize} 
			</if>
	</select>
	<select id="selectCount" resultType="java.lang.Integer" parameterType="map">
		select count(1) from redpigmall_goods_format obj 
		where 1 = 1  
		<include refid="whereStatement"/> 
	</select>
	<select id="queryPages" resultType="com.redpigmall.domain.GoodsFormat" resultMap="BaseResultMap" parameterType="map">
		select
		<include refid="Base_Column_List"/>
		from redpigmall_goods_format obj 			
		<where>
			1=1
			<include refid="whereStatement"/>
			<choose>
				<when test="orderBy != null and orderType != null">
					order by ${orderBy} ${orderType}
				</when>
				<otherwise>
					order by obj.addTime desc
				</otherwise>
			</choose>
		</where>
		<if test="currentPage !=null and pageSize !=null">
			limit #{currentPage},#{pageSize} 
		</if>
	</select>
	<insert id="saveEntity" parameterType="com.redpigmall.domain.GoodsFormat" keyProperty="id" keyColumn="id" useGeneratedKeys="true">
	insert into redpigmall_goods_format(addTime,deleteStatus,gf_name,gf_type,gf_content,gf_store_id,gf_cat)
	 values(#{addTime,jdbcType=TIMESTAMP},#{deleteStatus,jdbcType=INTEGER},#{gf_name,jdbcType=VARCHAR},#{gf_type,jdbcType=INTEGER},#{gf_content,jdbcType=VARCHAR},#{gf_store_id,jdbcType=BIGINT},#{gf_cat,jdbcType=BIGINT}
	)</insert>
	<update id="updateById" parameterType="com.redpigmall.domain.GoodsFormat">
		update redpigmall_goods_format
		<set>
			<include refid="ifStatement"/>
		</set>
		where id = #{id,jdbcType=BIGINT}
	</update>
	<delete id="deleteById" parameterType="java.lang.Long">
			delete from redpigmall_goods_format where id=#{id,jdbcType=BIGINT}
	</delete>
	<delete id="batchDeleteByIds" parameterType="java.util.List">
		delete from redpigmall_goods_format where id in
		<foreach collection="list" index="index" item="item" open="(" separator="," close=")">
			#{item}
		</foreach>
	</delete>
	<select id="queryPagesWithNoRelations" resultType="com.redpigmall.domain.GoodsFormat" resultMap="BaseResultMap2" parameterType="map">
		select
